{
    "$schema": "https://json.schemastore.org/package",
    "name": "app-template",
    "version": "1.0.0",
    "description": "toolkit for building apps",
    "author": "David Castner",
    "license": "Unlicense",
    "repository": "github:davidjcastner/app-template",
    "homepage": "https://github.com/davidjcastner/app-template",
    "scripts": {
        "build:development": "./node_modules/.bin/webpack --config ./webpack.config.ts --mode=development",
        "build:production": "./node_modules/.bin/webpack --config ./webpack.config.ts --mode=production",
        "lint:eslint": "./node_modules/.bin/eslint .",
        "start": "npm run watch:development",
        "watch:development": "./node_modules/.bin/webpack-dev-server --config ./webpack.config.ts --mode=development",
        "watch:production": "./node_modules/.bin/webpack-dev-server --config ./webpack.config.ts --mode=production"
    },
    "dependencies": {},
    "devDependencies": {
        "@types/html-webpack-plugin": "^3.2.4",
        "@types/webpack": "^4.41.23",
        "@types/webpack-dev-server": "^3.11.0",
        "@typescript-eslint/eslint-plugin": "^4.5.0",
        "@typescript-eslint/parser": "^4.5.0",
        "eslint": "^7.12.0",
        "html-webpack-plugin": "^4.5.0",
        "prettier": "^2.1.2",
        "ts-loader": "^8.0.7",
        "ts-node": "^9.0.0",
        "typescript": "^4.0.3",
        "webpack": "^5.2.0",
        "webpack-cli": "^3.3.12",
        "webpack-dev-server": "^3.11.0"
    }
}
